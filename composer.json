{
  "name": "williamcruzme/laravel-notification-settings",
  "type": "library",
  "description": "A Laravel package that allows you to check the notification settings before send them.",
  "keywords": [
    "laravel",
    "notification",
    "settings"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "williamcruzme",
      "email": "sv.johncruz@gmail.com",
      "homepage": "https://github.com/williamcruzme"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1.3",
    "illuminate/notifications": "~5.6 || ~6.0 || ~7.0 || ~8.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Millions\\Notifications\\NotificationServiceProvider"
      ],
      "aliases": {
        "Notification": "Millions\\Notifications\\Facades\\Notification"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Millions\\Notifications\\": "src/"
    }
  }
}
